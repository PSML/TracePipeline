#!/bin/bash

if [ $# -ne 3 ]; then
  echo "USAGE: $0 [exp] [img file] [output file]"
  exit 0
fi

exp=$1
img=$2
out=$3

/usr/bin/time -o time.info ~/6502/fibexp/bin/6502 -t sv -o $out $img $exp.oimg < ~/6502/fibexp/chess/input.random > $exp.stdout 2> $exp.stderr

echo "Created: $out | $out.stdout | $out.stderr | time.info"
