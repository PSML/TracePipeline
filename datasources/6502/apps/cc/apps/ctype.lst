000000r 2               
000000r 2               ; Define bitmapped constants for the table entries
000000r 2               
000000r 2               CT_NONE         = $00           ; Nothing special
000000r 2               CT_LOWER        = $01           ; 0 - Lower case char
000000r 2               CT_UPPER        = $02           ; 1 - Upper case char
000000r 2               CT_DIGIT        = $04           ; 2 - Numeric digit
000000r 2               CT_XDIGIT       = $08           ; 3 - Hex digit (both, lower and upper)
000000r 2               CT_CTRL         = $10           ; 4 - Control character
000000r 2               CT_SPACE        = $20           ; 5 - The space character itself
000000r 2               CT_OTHER_WS     = $40           ; 6 - Other whitespace ('\f', '\n', '\r', '\t' and '\v')
000000r 2               CT_SPACE_TAB    = $80           ; 7 - Space or tab character
000000r 2               
000000r 2               ; Combined stuff
000000r 2               CT_ALNUM        = (CT_LOWER | CT_UPPER | CT_DIGIT)
000000r 2               CT_ALPHA        = (CT_LOWER | CT_UPPER)
000000r 2               CT_CTRL_SPACE   = (CT_CTRL | CT_SPACE)
000000r 2               CT_NOT_PUNCT    = (CT_SPACE | CT_CTRL | CT_DIGIT | CT_UPPER | CT_LOWER)
000000r 2               
